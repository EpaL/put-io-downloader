{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "put-io-downloader",
  "title": "Put.io Downloader",
  "description": "Integrates with Put.io to provide an easy way to trigger file downloads",
  "icon": "command-icon.png",
  "author": "EpaL",
  "categories": [
    "Media",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Put.io Downloader",
      "subtitle": "Put.io Downloader",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@putdotio/api-client": "^8.32.0",
    "@raycast/api": "^1.35.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
